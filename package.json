    "@t3-oss/env-nextjs": "^0.13.6",
    "@tanstack/react-query": "^5.79.0",
    "@trpc/client": "^11.1.4",
    "@trpc/react-query": "^11.1.4",
    "@trpc/server": "^11.1.4",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "next": "^15.3.3",
    "next-auth": "5.0.0-beta.28",
